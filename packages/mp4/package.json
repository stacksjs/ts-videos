  "description": "MP4/MOV (ISOBMFF) codec support for ts-videos",
  "author": "Chris Breuer <chris@stacksjs.org>",
  "license": "MIT",
  "homepage": "https://github.com/stacksjs/ts-videos/tree/main/packages/mp4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/ts-videos.git",
    "directory": "packages/mp4"
  },
